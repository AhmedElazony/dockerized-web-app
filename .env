# Application URLs
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:4200

# Ports
BACKEND_PORT=8000
FRONTEND_PORT=4200
DATABASE_PORT=3306
REDIS_PORT=6379
